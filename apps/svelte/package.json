{
	"name": "@tinyslice/svelte-example",
	"version": "0.8.0",
	"archetype": {
		"platform": "web",
		"framework": "sveltekit",
		"language": "ts",
		"kind": "app"
	},
	"keywords": [
		"managed-by-autotool",
		"svelte"
	],
	"type": "module",
	"scripts": {
		"build": "turbo run build-app_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"build-app_": "vite build",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 6 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:es_": "eslint --max-warnings=0 --fix --no-error-on-unmatched-pattern .",
		"lint:format": "turbo run lint:format_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --check .",
		"lint:md": "turbo run lint:md_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:md_": "remark --frail --no-stdout --silently-ignore .",
		"lint:style": "turbo run lint:style_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:style:css": "turbo run lint:style:css_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:style:css_": "stylelint --cache true --cache-strategy content --cache-location .cache/stylelintcache-css --allow-empty-input '**/*.css'",
		"lint:style:html": "turbo run lint:style:html_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:style:html_": "stylelint --customSyntax=postcss-html --cache true --cache-strategy content --cache-location .cache/stylelintcache-html --allow-empty-input '**/*.{html,svelte,vue,astro,xml,php}'",
		"lint:style:scss": "turbo run lint:style:scss_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:style:scss_": "stylelint --customSyntax=postcss-scss --cache true --cache-strategy content --cache-location .cache/stylelintcache-scss --allow-empty-input '**/*.scss'",
		"lint:svelte": "turbo run lint:svelte_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:svelte_": "svelte-check --tsconfig ./tsconfig.json",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"lint:tsc_": "tsc --noEmit",
		"all": "BUILD_REASON='publish' turbo run all_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"dev": "concurrently npm:watch-deps npm:start",
		"format": "turbo run format_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"format_": "prettier --cache-location .cache/prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --write .",
		"postinstall": "svelte-kit sync # pakk:not-distributed",
		"start": "TARGET_ENV='local' vite",
		"test": "turbo run test_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example",
		"test_": "vitest --passWithNoTests --coverage --run",
		"test:watch": "vitest --passWithNoTests --coverage",
		"watch-deps": "TARGET_ENV='local' nodemon --config ../../node_modules/@alexaegis/autotool-plugin-vite/static/nodemon.json --watch ./node_modules/**/src/**/* --ext ts,tsx,mts,cts,svelte,js,jsx,mjs,cjs,json --ignore dist --exec 'turbo run build-lib_ --concurrency 16 --cache-dir .cache/turbo --filter @tinyslice/svelte-example'"
	},
	"dependencies": {
		"@tinyslice/core": "workspace:^",
		"@tinyslice/devtools-plugin": "workspace:^",
		"@tinyslice/hydration-plugin": "workspace:^",
		"@tinyslice/logger-plugin": "workspace:^",
		"rxjs": "^7.8.1",
		"svelte-rxjs-observer": "^0.4.0",
		"tailwindcss": "^3.3.3"
	},
	"devDependencies": {
		"@alexaegis/eslint-config-svelte": "^0.5.12",
		"@alexaegis/eslint-config-vitest": "^0.5.12",
		"@alexaegis/stylelint-config": "^0.5.12",
		"@alexaegis/svelte-config": "^0.5.12",
		"@alexaegis/ts": "^0.5.12",
		"@alexaegis/vite": "^0.5.12",
		"@alexaegis/vitest": "^0.5.12",
		"@skeletonlabs/skeleton": "^1.10.0",
		"@sveltejs/kit": "^1.22.3",
		"@types/node": "^20.4.4",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.2.0",
		"eslint-config-next": "^13.4.12",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-turbo": "^1.10.9",
		"eslint-plugin-react": "^7.33.0",
		"eslint-plugin-svelte": "^2.32.4",
		"eslint-plugin-unicorn": "^48.0.0",
		"nodemon": "^3.0.1",
		"stylelint": "^15.10.2",
		"svelte": "^4.1.1",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"vite": "^4.4.6",
		"vitest": "^0.33.0"
	}
}
