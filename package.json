{
	"name": "tinyslice",
	"description": "Reactive events",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/tinyslice#readme",
	"repository": "github:AlexAegis/tinyslice",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/tinyslice/issues"
	},
	"keywords": [
		"tinyslice",
		"cqrs",
		"actions",
		"effects",
		"rxjs",
		"reactive",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"workspaces": [
		"libs/core",
		"libs/angular",
		"libs/devtools-plugin",
		"examples/angular",
		"examples/svelte"
	],
	"scripts": {
		"start:svelte-example": "npm --workspace examples/svelte start",
		"start:angular-example": "nx run angular:serve",
		"format": "prettier --write .",
		"lint:es": "eslint",
		"lint:format": "prettier --check .",
		"lint:style": "stylecheck",
		"test:jest": "jest --passWithNoTests",
		"ncu": "ncu -u",
		"clean": "rimraf node_modules package-lock.json",
		"ng": "nx",
		"postinstall": "husky install && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
		"start": "nx serve",
		"build": "nx build",
		"test": "nx test"
	},
	"dependencies": {
		"@angular/animations": "~13.3.11",
		"@angular/common": "~13.3.11",
		"@angular/compiler": "~13.3.11",
		"@angular/core": "~13.3.11",
		"@angular/forms": "~13.3.11",
		"@angular/platform-browser": "~13.3.11",
		"@angular/platform-browser-dynamic": "~13.3.11",
		"@angular/router": "~13.3.11",
		"@nrwl/angular": "14.1.9",
		"rxjs": "~7.5.5",
		"tslib": "^2.4.0",
		"zone.js": "~0.11.5"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~13.3.7",
		"@angular-eslint/eslint-plugin": "~13.2.1",
		"@angular-eslint/eslint-plugin-template": "~13.2.1",
		"@angular-eslint/template-parser": "~13.2.1",
		"@angular/cli": "~13.3.7",
		"@angular/compiler-cli": "~13.3.11",
		"@angular/language-service": "~13.3.11",
		"@commitlint/cli": "~17.0.2",
		"@commitlint/config-conventional": "~17.0.2",
		"@nrwl/cli": "14.1.9",
		"@nrwl/cypress": "14.1.9",
		"@nrwl/eslint-plugin-nx": "14.1.9",
		"@nrwl/jest": "14.1.9",
		"@nrwl/linter": "14.1.9",
		"@nrwl/workspace": "14.1.9",
		"@types/jest": "^27.0.0",
		"@types/node": "17.0.38",
		"@typescript-eslint/eslint-plugin": "~5.18.0",
		"@typescript-eslint/parser": "~5.18.0",
		"babel-jest": "^28.1.0",
		"cypress": "^9.0.0",
		"dotenv": "~16.0.1",
		"eslint": "~8.16.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~4.0.0",
		"husky": "^8.0.1",
		"jest": "^28.1.0",
		"jest-environment-jsdom": "^28.1.0",
		"jest-preset-angular": "next",
		"lint-staged": "~13.0.0",
		"markdownlint": "^0.25.1",
		"markdownlint-cli": "^0.31.1",
		"npm-check-updates": "~13.0.4",
		"nx": "14.1.9",
