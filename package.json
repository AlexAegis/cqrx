{
	"name": "tinyslice",
	"description": "Reactive state management made tiny",
	"version": "0.2.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/tinyslice#readme",
	"repository": "github:AlexAegis/tinyslice",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/tinyslice/issues"
	},
	"keywords": [
		"tinyslice",
		"cqrs",
		"actions",
		"effects",
		"rxjs",
		"reactive",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"workspaces": [
		"libs/core",
		"libs/angular",
		"libs/devtools-plugin",
		"examples/angular",
		"examples/svelte"
	],
	"scripts": {
		"serve:angular": "nx run angular:serve",
		"serve:svelte": "nx run svelte:serve",
		"format": "prettier --write .",
		"lint": "nx run-many --parallel 4 --target lint",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:format": "prettier --check .",
		"lint:style": "stylecheck",
		"test:jest": "jest --passWithNoTests",
		"ncu": "ncu -u",
		"clean": "rimraf node_modules package-lock.json",
		"ng": "nx",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"postinstall": "husky install && node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"start": "nx serve",
		"build": "nx run-many --parallel 4 --target build --configuration production",
		"test": "nx run-many --parallel 4 --target test --coverage"
	},
	"dependencies": {
		"@angular/animations": "~14.2.4",
		"@angular/common": "~14.2.4",
		"@angular/compiler": "~14.2.4",
		"@angular/core": "~14.2.4",
		"@angular/forms": "~14.2.4",
		"@angular/platform-browser": "~14.2.4",
		"@angular/platform-browser-dynamic": "~14.2.4",
		"@angular/router": "~14.2.4",
		"@nrwl/angular": "~14.8.0",
		"@nxext/svelte": "^14.0.2",
		"@nxext/vite": "15.0.0-alpha.5",
		"ng-zorro-antd": "^14.0.0",
		"rxjs": "7.5.7",
		"tslib": "^2.4.0",
		"zone.js": "~0.11.8"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~14.2.4",
		"@angular-devkit/schematics": "~14.2.4",
		"@angular-eslint/eslint-plugin": "~14.1.2",
		"@angular-eslint/eslint-plugin-template": "~14.1.2",
		"@angular-eslint/template-parser": "~14.1.2",
		"@angular/cli": "~14.2.4",
		"@angular/compiler-cli": "~14.2.4",
		"@angular/language-service": "~14.2.4",
		"@commitlint/cli": "~17.1.2",
		"@commitlint/config-conventional": "~17.1.0",
		"@nrwl/cli": "14.8.0",
		"@nrwl/cypress": "14.8.0",
		"@nrwl/eslint-plugin-nx": "14.8.0",
		"@nrwl/jest": "14.8.0",
		"@nrwl/js": "14.8.0",
		"@nrwl/linter": "14.8.0",
		"@nrwl/workspace": "14.8.0",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/kit": "1.0.0-next.505",
		"@sveltejs/vite-plugin-svelte": "^1.0.8",
		"@testing-library/svelte": "^3.2.1",
		"@tsconfig/svelte": "^3.0.0",
		"@types/jest": "^29.0.3",
		"@types/node": "18.7.23",
		"@typescript-eslint/eslint-plugin": "5.38.1",
		"@typescript-eslint/parser": "5.38.1",
		"babel-jest": "^29.1.0",
		"c8": "^7.12.0",
		"cypress": "^10.9.0",
		"dotenv": "~16.0.2",
		"eslint": "8.24.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~4.2.1",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^29.1.1",
		"jest-environment-jsdom": "^29.1.1",
		"jest-preset-angular": "12.2.2",
		"lint-staged": "~13.0.3",
		"markdownlint": "^0.26.2",
		"markdownlint-cli": "^0.32.2",
		"ng-packagr": "~14.2.1",
		"npm-check-updates": "~16.3.3",
		"nx": "14.8.0",
		"pinst": "^3.0.0",
		"postcss": "^8.4.16",
		"postcss-import": "~15.0.0",
		"postcss-preset-env": "~7.8.2",
		"postcss-url": "~10.1.3",
		"prettier": "2.7.1",
		"prettier-stylelint": "^0.4.2",
		"regenerator-runtime": "^0.13.9",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.21",
		"standard-version": "^9.5.0",
		"stylelint": "^14.13.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^28.0.0",
		"stylelint-prettier": "^2.0.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.7",
		"ts-jest": "^29.0.2",
		"ts-node": "10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "4.8.4",
		"vitest": "^0.23.4",
		"webpack-bundle-analyzer": "~4.6.1"
	}
}
