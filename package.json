{
	"name": "cqrx-workspace",
	"description": "Reactive events",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/cqrx#readme",
	"repository": "github:AlexAegis/cqrx",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/cqrx/issues"
	},
	"keywords": [
		"cqrx",
		"cqrs",
		"actions",
		"effects",
		"rxjs",
		"reactive",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"workspaces": [
		"lib/cqrx",
		"example/svelte-example"
	],
	"scripts": {
		"start": "npm --workspace svelte-example start",
		"format": "prettier --write .",
		"lint:es": "eslint",
		"lint:format": "prettier --check .",
		"test": "jest --passWithNoTests",
		"ncu": "ncu -u",
		"clean": "rimraf node_modules package-lock.json",
		"_postinstall": "husky install",
		"prepublish": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"dependencies": {
		"rxjs": "7.5.4",
		"tslib": "^2.3.1"
	},
	"devDependencies": {
		"@commitlint/cli": "~16.2.1",
		"@commitlint/config-conventional": "~16.2.1",
		"@types/jest": "~27.4.0",
		"@types/node": "17.0.18",
		"@typescript-eslint/eslint-plugin": "~5.12.0",
		"@typescript-eslint/parser": "~5.12.0",
		"dotenv": "~16.0.0",
		"eslint": "~8.9.0",
		"eslint-config-prettier": "~8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~4.0.0",
		"husky": "^7.0.4",
		"jest": "~27.5.1",
		"lint-staged": "~12.3.4",
		"markdownlint": "^0.25.1",
		"markdownlint-cli": "^0.31.1",
		"npm-check-updates": "~12.3.0",
		"pinst": "^2.1.6",
		"prettier": "~2.5.1",
		"rimraf": "^3.0.2",
		"source-map-support": "^0.5.21",
		"ts-node": "10.5.0",
		"tsconfig-paths": "^3.12.0",
		"typescript": "~4.5.5",
		"webpack-bundle-analyzer": "~4.5.0"
	}
}
